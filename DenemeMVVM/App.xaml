        <Style x:Key="Table"  TargetType="Button">
            <Setter Property="Height" Value="90"/>
            <Setter Property="Width" Value="120"/>
            <Setter Property="FontSize" Value="20"/>
        </Style>
    </Application.Resources>
</Application>
